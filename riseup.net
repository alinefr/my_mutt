set imap_user="aline@riseup.net"
set imap_pass=`gkeyring -k login -p 'user=aline,protocol=imap' -o secret`
set smtp_url=smtps://$imap_user:$imap_pass@mail.riseup.net
set from=$imap_user
set folder=imaps://mail.riseup.net/
set spoolfile=+Inbox
set postponed=+Drafts
set header_cache="~/.cache/mutt"
set message_cachedir="~/.cache/mutt/messages"
set record=+Sent
